# OpenIntegrationEngine Configuration

# Database Configuration  
DATABASE=postgres
DATABASE_URL=jdbc:postgresql://database:5432/mirthdb
DATABASE_USERNAME=mirthuser
DATABASE_PASSWORD=mirthpass

# PostgreSQL Database Settings
POSTGRES_DB=mirthdb
POSTGRES_USER=mirthuser
POSTGRES_PASSWORD=mirthpass

# Keystore Configuration
KEYSTORE_STOREPASS=changeit
KEYSTORE_KEYPASS=changeit

# JVM Options
VMOPTIONS=-Xmx512m

# Database Connection Pool
DATABASE_MAX_CONNECTIONS=20