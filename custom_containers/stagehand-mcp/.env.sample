# Browserbase API Configuration
# Sign up at https://browserbase.com to get your API key and project ID
BROWSERBASE_API_KEY=your_browserbase_api_key_here
BROWSERBASE_PROJECT_ID=your_browserbase_project_id_here

# OpenAI API Configuration (required by the MCP server)
# Get your API key from https://platform.openai.com
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Debug and logging
DEBUG=false
LOG_LEVEL=info

# Optional: Server configuration
MCP_SERVER_NAME=stagehand-browser-automation
MCP_SERVER_VERSION=1.0.0